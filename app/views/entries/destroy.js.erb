<% if @entry.destroyed? %>
<%# render success message %>
$('#alerts').html("<%= j render partial: 'alert_destroy_success', locals: { time: @entry.time } %>");
<%# remove table row %>
$('#entry_<%= j @entry.id %>').remove();
$('#notes_<%= j @entry.id %>').remove();
<%# decrement "number of readings" %>
$("#num_readings_<%= @entry.time.to_date.to_formatted_s(:number)%>").each(function() {
  $(this).text(String(parseInt($(this).text(), 10) - 1));
});
<% else %>
<%# render error message %>
$('#alerts').html("<%= j render partial: 'alert_destroy_error' %>");
<% end %>
