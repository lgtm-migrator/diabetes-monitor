%h1 Log entries

= link_to share_tokens_url, class: 'btn btn-info mb-2 mt-2' do
  %i.bi.bi-share.mr-1
  Share

.table-responsive.border.border-dark
  %table.table.table-hover
    = render 'table_head'

    %tbody
      - @data.keys.sort.reverse_each do |date|
        = render partial: 'day', locals: { date: date, entries: @data[date] }
